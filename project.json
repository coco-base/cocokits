{
  "name": "cocokits",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "list": {
      "executor": "@cocokits/workspace-plugin:pnpm-workspace-list",
      "options": {
        "details": false
      },
      "configurations": {
        "details": {
          "details": true
        }
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --all --target=lint --parallel --maxParallel=4 --exclude=cocokits"
      }
    }
  }
}
