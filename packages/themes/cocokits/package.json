{
  "name": "@cocokits/theme-cocokits",
  "version": "1.1.0",
  "private": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./index.js",
      "require": "./index.js",
      "module": "./index.js",
      "main": "./index.js"
    },
    "./styles.css": "./src/styles/css/components.css",
    "./styles.min.css": "./src/styles/css/components.min.css",
    "./tokens.css": "./src/token/css/tokens.css",
    "./tokens.min.css": "./src/token/css/tokens.min.css",
    "./tokens": {
      "sass": "./_tokens.scss"
    },
    "./styles-core": {
      "sass": "./_styles-core.scss"
    },
    "./tokens-core": {
      "sass": "./_tokens-core.scss"
    }
  },
  "dependencies": {
    "@cocokits/core": "workspace:*",
    "@swc/helpers": "~0.5.2"
