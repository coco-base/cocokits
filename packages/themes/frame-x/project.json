{
  "name": "@cocokits/theme-frame-x",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/themes/frame-x/src",
  "projectType": "library",
  "tags": ["type:theme", "framework:shared"],
  "targets": {
    "build": {
      "executor": "@cocokits/workspace-plugin:scss-builder",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/themes/frame-x",
        "sourceMap": true,
        "css": ["packages/themes/frame-x/src/styles.scss", "packages/themes/frame-x/src/token.scss"],
        "min": ["packages/themes/frame-x/src/styles.scss", "packages/themes/frame-x/src/token.scss"]
      }
    },
    "token-generator": {
      "executor": "@cocokits/workspace-plugin:token-generator",
      "outputs": ["{options.outputPath}"],
      "options": {
        "files": ["packages/themes/frame-x/json-token/manifest.json"],
        "excludeCollections": ["Breakpoints", "┈┈┈┈┈┈┈┈┈", "Components"],
        "fontWeightPattern": "font-weight/*",
        "parser": "design-tokens-manager",
        "outputDir": "packages/themes/frame-x/src/token",
        "themeName": "frame-x"
      }
    },
    "quick-fix-eslint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
