{
  "shadow": {
    "xs": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "2px",
          "spread": "0px",
          "color": "#1a1a1a14"
        }
      ]
    },
    "sm": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "0.5px",
          "blur": "0px",
          "spread": "0px",
          "color": "#1a1a1a14"
        },
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "3px",
          "spread": "0px",
          "color": "#1a1a1a14"
        }
      ]
    },
    "md": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "0px",
          "spread": "0px",
          "color": "#1a1a1a14"
        },
        {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "-1px",
          "color": "#1a1a1a14"
        }
      ]
    },
    "lg": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1.5px",
          "blur": "0px",
          "spread": "0.5px",
          "color": "#1a1a1a14"
        },
        {
          "offsetX": "0px",
          "offsetY": "8px",
          "blur": "16px",
          "spread": "-3px",
          "color": "#1a1a1a14"
        }
      ]
    },
    "xl": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "1px",
          "color": "#1a1a1a14"
        },
        {
          "offsetX": "0px",
          "offsetY": "16px",
          "blur": "40px",
          "spread": "-5px",
          "color": "#1a1a1a14"
        }
      ]
    },
    "2xl": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "2.5px",
          "blur": "3px",
          "spread": "2px",
          "color": "#1a1a1a14"
        },
        {
          "offsetX": "0px",
          "offsetY": "16px",
          "blur": "64px",
          "spread": "-12px",
          "color": "#1a1a1a29"
        }
      ]
    },
    "inner": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "0px",
          "color": "#1a1a1a1f",
          "inset": true
        }
      ]
    },
    "flat": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "0px",
          "spread": "0px",
          "color": "#e1e1e2"
        }
      ]
    }
  },
  "blur": {
    "xs": {
      "$type": "shadow",
      "$value": [
        {
          "backgroundBlue": true,
          "blur": "8px"
        }
      ]
    },
    "sm": {
      "$type": "shadow",
      "$value": [
        {
          "backgroundBlue": true,
          "blur": "16px"
        }
      ]
    },
    "md": {
      "$type": "shadow",
      "$value": [
        {
          "backgroundBlue": true,
          "blur": "24px"
        }
      ]
    },
    "lg": {
      "$type": "shadow",
      "$value": [
        {
          "backgroundBlue": true,
          "blur": "40px"
        }
      ]
    },
    "test": {
      "$type": "color",
      "$value": { "value": "rgba(100,200,255, 1)", "alpha": 0.2 }
    }
  }
}
