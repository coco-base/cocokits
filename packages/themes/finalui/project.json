        "sourceMap": true,
        "css": ["packages/themes/finalui/src/index.scss"],
        "min": ["packages/themes/finalui/src/index.scss"]
      }
    },
    "eslint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
