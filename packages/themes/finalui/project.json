{
  "name": "finalui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/themes/finalui/src",
  "projectType": "library",
  "tags": ["type:theme", "framework:shared"],
  "targets": {
    "build": {
      "executor": "@coco-kits/workspace-plugin:scss-builder",
      "outputs": ["{options.outputPath}"],
      "options": {
