{
  "name": "@cocokits/theme-default",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/themes/default/src",
  "projectType": "library",
  "tags": ["type:theme", "framework:shared"],
  "targets": {
    "build": {
      "executor": "@cocokits/workspace-plugin:scss-builder",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/themes/default",
        "sourceMap": true,
        "css": ["packages/themes/default/src/styles.scss", "packages/themes/default/src/token/scss/index.scss"],
        "min": ["packages/themes/default/src/styles.scss", "packages/themes/default/src/token/scss/index.scss"]
      }
    },
    "token-generator": {
      "executor": "@cocokits/workspace-plugin:token-generator",
      "outputs": ["{options.outputPath}"],
      "options": {
        "files": ["packages/themes/default/json-token/manifest.json"],
        "excludeCollections": [],
        "parser": "design-tokens-manager",
        "outputDir": "packages/themes/default/src/token",
        "themeName": "default"
      }
    },
    "quick-fix-eslint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
